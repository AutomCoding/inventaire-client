<div class="patch-operations">
  {{#each this}}
    <div class="patch patch-{{op}}">
      <div class="op">{{op}}</div>
      <div class="path" title="{{path}}">{{propertyLabel}}</div>
      {{#if value}}<div class="value">{{{multiTypeValue value}}}</div>{{/if}}
      {{#if filter}}<a class="filter" href="{{filterPathname}}">{{filter}}</a>{{/if}}
    </div>
  {{/each}}
</div>
