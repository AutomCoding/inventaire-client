<div class="controls">
  <h2>{{serie.label}}</h2>

  <div class="control">
    <label for="partsNumber">{{i18n 'number of parts'}}</label>
    <select id="partsNumber">
      {{#each partsNumberPickerRange}}
        <option value="{{this}}">{{this}}</option>
      {{/each}}
    </select>
  </div>

  <a id="save" class="tiny-success-button">{{I18n 'save'}}</a>
</div>

<div id="conflicts"></div>
<div id="withoutOrdinal"></div>
<div id="withOrdinal"></div>
