{{#unless hideHeader}}
  <div class="header">
    <h3>{{i18n title}}</h3>
    <span class="counter">{{totalLength}}</span>
  </div>
{{/unless}}
<div class="container"></div>
{{#if more}}
  <div class="more">
    <a class="displayMore">
      {{icon 'chevron-down'}}{{i18n 'more'}}
      <span class="counter">{{more}}</span>
    </a>
  </div>
{{/if}}
