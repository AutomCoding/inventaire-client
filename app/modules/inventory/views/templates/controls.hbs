<a class="showControls">
  {{icon 'wrench'}}
  {{icon 'chevron-right'}}
</a>
<div class="innerControls">
  <div class="layouts">
    <a class="cascade">{{icon 'th-large'}}</a>
    <a class="grid">{{icon 'align-justify'}}</a>
  </div>
  <div class="transactions">
    {{#each transactions}}
      <a class="{{id}} transaction active" title="{{title}}">{{icon icon}}</a>
    {{/each}}
  </div>
  <input type="text" class="filter" placeholder="filter...">
</div>
