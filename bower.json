    "foundation": "https://github.com/maxlath/bower-foundation.git#brunch-custom",
    "font-awesome": "~4.3.0",
    "polyglot": "~0.4.1",
    "backbone-filtered-collection": "~0.4.0",
    "backbone-nested-afeld": "~2.0.1",
    "lang": "https://github.com/maxlath/lang.git",
    "qlabel": "https://github.com/maxlath/qlabel.git#bower-support",
    "blueimp-canvas-to-blob": "~2.1.1",
    "moment": "~2.8.4",
    "bluebird": "~2.6.4",
    "jquery.inview": "~1.0.0",
    "imagesloaded": "~3.1.8",
    "jquery-elastic": "git+https://github.com/SebastianEdwards/jquery.elastic.js.git",
    "jquery-visible": "~1.1.0",
