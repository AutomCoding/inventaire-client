{
  "author": "maxlath",
  "name": "inventaire-client",
  "description": "resources mapper",
  "version": "0.0.1",
  "homepage": "http://inventaire.io",
  "repository": {
    "type": "git",
    "url": "http://github.com/maxlath/inventaire-client"
  },
  "engines": {
    "node": "0.10"
  },
  "scripts": {
    "postinstall": "npm run-script browserify-modules",
    "prepare-production": "bower install -p && ./scripts/modernizr-bower-fix && brunch build -P",
    "start": "brunch watch",
    "server": "brunch watch --server",
    "test": "mocha --compilers coffee:coffee-script/register --watch",
    "lint": "coffeelint app/* test/* brunch-config.coffee",
    "browserify-modules": "./scripts/browserify-node-modules"
  },
  "dependencies": {
    "auto-reload-brunch": "1.7",
    "browserify": "^6.2.0",
    "coffee-script-brunch": "1.8",
    "css-brunch": "1.7",
    "handlebars-brunch": "1.7",
    "javascript-brunch": "1.7",
    "level-js": "^2.1.6",
    "level-multiply": "0.0.1",
    "level-promise": "^1.3.1",
    "levelup": "^0.19.0",
    "sass-brunch": "1.8",
    "uglify-js-brunch": "1.7"
  },
  "devDependencies": {}
}