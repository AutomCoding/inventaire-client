{
  "author": "maxlath",
  "name": "inventaire-client",
  "description": "resources mapper",
  "version": "0.0.1",
  "homepage": "http://inventaire.io",
  "repository": {
    "type": "git",
    "url": "http://github.com/maxlath/inventaire-client"
  },
  "engines": {
    "node": "0.10"
  },
  "scripts": {
    "postinstall": "npm run slim-packages; npm run install-scripts; bower install; npm run bower-fix",
    "prepare-production": "brunch build -P",
    "start": "brunch watch",
    "server": "brunch watch --server",
    "test": "mocha --compilers coffee:coffee-script/register --watch",
    "lint": "coffeelint app/* test/* brunch-config.coffee",
    "slim-packages": "coffee ./scripts/slim_packages.coffee",
    "install-scripts": "./scripts/install_scripts",
    "bower-fix": "coffee ./scripts/bower_fix.coffee",
    "update-i18n": "coffee ./scripts/generate_lang_json.coffee"
  },
  "dependencies": {
    "auto-reload-brunch": "1.7",
    "browserify": "^6.2.0",
    "clean-css-brunch": "^1.7.1",
    "coffee-script-brunch": "1.8",
    "css-brunch": "1.7",
    "git-digest-brunch": "^1.0.3",
    "handlebars-brunch": "1.7",
    "javascript-brunch": "1.7",
    "level-js": "^2.1.6",
    "level-multiply": "0.0.1",
    "level-promise": "^1.3.1",
    "levelup": "^0.19.0",
    "sass-brunch": "1.8",
    "uglify-js-brunch": "1.7"
  },
  "devDependencies": {
    "handlebars": "^2.0.0"
  }
}
