    "brunch": "^2.5.3",
    "chai": "^3.5.0",
    "clean-css-brunch": "^2.0.0",
    "coffee-script-brunch": "^2.0.0",
    "coffeeify": "^2.0.1",
    "config": "^1.19.0",
    "css-brunch": "^2.0.0",
    "csvtojson": "^0.5.2",
    "fb-flo": "^0.5.0",
    "git-digest-brunch": "^1.0.3",
    "git-hooks": "^1.0.2",
    "graceful-fs": "^4.1.2",
    "handlebars": "^4.0.5",
    "handlebars-brunch": "^2.0.1",
    "html-minifier": "^1.1.1",
    "javascript-brunch": "^2.0.0",
    "lodash": "^4.6.1",
    "ls": "^0.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "sass-brunch": "^2.0.0",
    "sinon": "^1.17.3",
    "transifexify": "^1.0.6",
    "uglify-js-brunch": "^2.0.1"
