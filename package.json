{
  "author": "maxlath",
  "name": "inventaire-client",
  "description": "a libre collaborative resources mapper powered by open-knowledge (client-side)",
  "version": "2.1.0",
  "license": "AGPL-3.0",
  "homepage": "https://inventaire.io",
  "repository": {
    "type": "git",
    "url": "http://github.com/maxlath/inventaire-client"
  },
  "engines": {
    "node": ">= 6.4"
  },
  "scripts": {
    "build": "./scripts/build",
    "edit-mentions": "libreoffice ./scripts/assets/mentions.csv && ./scripts/update_mentions.coffee",
    "generate-sitemaps": "./scripts/sitemaps/generate.coffee",
    "lint": "coffeelint -f ./config/coffeelint.json --color always app test scripts",
    "lint-staged": "../scripts/lint-staged",
    "patches": "./scripts/run_patches ./scripts/patches",
    "postinstall": "./scripts/postinstall",
    "start": "brunch watch",
    "test": "mocha --compilers coffee:coffee-script/register --exit --no-deprecation",
    "test-build": "./scripts/test_build/run local",
    "test-build-prod": "./scripts/test_build/run prod",
    "test-pages-changes": "./scripts/changes/test_pages_changes",
    "test-watch": "mocha --compilers coffee:coffee-script/register --watch --exit --no-deprecation",
    "update-i18n": "./scripts/update_i18n",
    "update-toc": "doctoc README.md",
    "watch": "brunch watch"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "bluebird": "^3.5.3",
    "cropper": "^2.3.0",
    "enumerate-devices": "^1.1.0",
    "fork-awesome": "^1.1.5",
    "isbn2": "^0.1.8",
    "leaflet": "^1.3.1",
    "leaflet.markercluster": "^1.3.0",
    "leven": "^2.1.0",
    "papaparse": "^4.1.2",
    "quagga": "^0.12.1",
    "wikidata-lang": "^2.0.8"
  },
  "devDependencies": {
    "after-brunch": "0.0.5",
    "auto-reload-brunch": "^2.7.1",
    "autoprefixer": "^6.7.7",
    "bluereq": "^2.1.7",
    "bower": "^1.7.7",
    "browserify": "^11.2.0",
    "brunch": "^2.10.17",
    "chalk": "^1.1.3",
    "clean-css": "^3.4.28",
    "clean-css-brunch": "^2.10.0",
    "coffee-script": "^1.12.2",
    "coffee-script-brunch": "^2.1.0",
    "coffeeify": "^2.0.1",
    "coffeelint": "^1.16.0",
    "config": "^1.19.0",
    "csvtojson": "^0.5.2",
    "doctoc": "^1.3.0",
    "git-digest-brunch": "^1.0.3",
    "git-hooks": "^1.0.2",
    "handlebars": "^4.0.11",
    "handlebars-brunch": "^2.10.0",
    "html-minifier": "^1.5.0",
    "iconv-lite": "^0.4.15",
    "lodash": "^4.17.10",
    "mocha": "^5.0.0",
    "postcss-brunch": "^0.5.0",
    "sass-brunch": "^2.10.4",
    "should": "^13.2.1",
    "sinon": "^1.17.3",
    "uglify-js": "^3.3.9",
    "uglify-js-brunch": "^2.10.0",
    "underscore": "^1.8.3",
    "wikidata-cli": "^3.0.6",
    "wikidata-sdk": "^5.15.8"
  }
}
